  <Definition xsi:type="MyObjectBuilder_PhysicsBodyComponentDefinition">
    <Id Type="PhysicsBodyComponent" Subtype="FracturedPiece" />
    <RigidBodyFlags>RBF_DEBRIS</RigidBodyFlags>
    <CollisionLayer>FracturePieceLayer</CollisionLayer>
    <MassPropertiesComputation>Box</MassPropertiesComputation>
    <AngularDamping>2</AngularDamping>
    <CreateFromCollisionObject>true</CreateFromCollisionObject>
    <ForceActivate>true</ForceActivate>
    <UpdateFlags>Gravity</UpdateFlags>
    <SerializePhysics>true</SerializePhysics>
		<PhysicalMaterial>Stone</PhysicalMaterial>
    <SuppressWarning>true</SuppressWarning><!-- Suppresses warning in log for invalid model. This is correct for Fracture Pieces. -->
  </Definition>
	
  <Definition xsi:type="MyObjectBuilder_PhysicsBodyComponentDefinition">
    <Id Type="PhysicsBodyComponent" Subtype="FracturedPieceStatic" />
    <RigidBodyFlags>RBF_STATIC</RigidBodyFlags>
    <CollisionLayer>FracturePieceLayer</CollisionLayer>
    <MassPropertiesComputation>Box</MassPropertiesComputation>
    <CreateFromCollisionObject>true</CreateFromCollisionObject>
    <ForceActivate>true</ForceActivate>
    <SerializePhysics>true</SerializePhysics>
		<PhysicalMaterial>Stone</PhysicalMaterial>
    <SuppressWarning>true</SuppressWarning><!-- Suppresses warning in log for invalid model. This is correct for Fracture Pieces. -->
  </Definition>
	
  <Definition xsi:type="MyObjectBuilder_PhysicsBodyComponentDefinition">
    <Id Type="PhysicsBodyComponent" Subtype="Tree" />
    <RigidBodyFlags>RBF_DEFAULT</RigidBodyFlags>
    <CollisionLayer>FloatingObjectCollisionLayer</CollisionLayer>
    <MassPropertiesComputation>Box</MassPropertiesComputation>
		<LinearDamping>0.4</LinearDamping>
    <AngularDamping>0.3</AngularDamping>
    <CreateFromCollisionObject>true</CreateFromCollisionObject>
    <ForceActivate>true</ForceActivate>
    <UpdateFlags>Gravity</UpdateFlags>
    <SerializePhysics>true</SerializePhysics>
		<PhysicalMaterial>Wood</PhysicalMaterial>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_FloatingObjectDecayComponentDefinition">
    <Id Type="FloatingObjectDecayComponent" Subtype="Default" />
