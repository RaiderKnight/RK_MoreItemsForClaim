<?xml version="1.0" encoding="utf-8"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!--Definition for Cube Builder Component (Building cubes)-->
  <Definition xsi:type="MyObjectBuilder_CubeBuilderDefinition">
    <Id Type="MyObjectBuilder_CubeBuilderDefinition" Subtype="CubeBuilderBase" />

    <!--Distance settings-->
    <DefaultBlockBuildingDistance>10</DefaultBlockBuildingDistance>
    <MaxBlockBuildingDistance>20</MaxBlockBuildingDistance>
    <MinBlockBuildingDistance>0.25</MinBlockBuildingDistance>
    <MinBuildingDistLargeSurvivalCharacter>0.25</MinBuildingDistLargeSurvivalCharacter>
    <MaxBuildingDistLargeSurvivalCharacter>6</MaxBuildingDistLargeSurvivalCharacter>
    <MinBuildingDistSmallSurvivalCharacter>1</MinBuildingDistSmallSurvivalCharacter>
    <MaxBuildingDistSmallSurvivalCharacter>6</MaxBuildingDistSmallSurvivalCharacter>

    <!--Placement settings-->
    <BuildingSettings>
      <StaticGridAlignToCenter>false</StaticGridAlignToCenter>
      <SmallStaticGrid>
        <SnapMode>OneFreeAxis</SnapMode>
        <SearchHalfExtentsDeltaRatio>0</SearchHalfExtentsDeltaRatio>
        <SearchHalfExtentsDeltaAbsolute>0</SearchHalfExtentsDeltaAbsolute>
        <CanAnchorToStaticGrid>false</CanAnchorToStaticGrid>
        <EnablePreciseRotationWhenSnapped>false</EnablePreciseRotationWhenSnapped>
        <!--Voxel Placement Settings: Possible settings Both,InVoxel,OutsideVoxel,Volumetric. If volumetric set than MaxAllowed and MinAllowed will be used.-->
        <VoxelPlacement>
          <PlacementMode>OutsideVoxel</PlacementMode>
        </VoxelPlacement>
      </SmallStaticGrid>
      <SmallGrid>
        <SnapMode>OneFreeAxis</SnapMode>
        <SearchHalfExtentsDeltaRatio>0</SearchHalfExtentsDeltaRatio>
        <SearchHalfExtentsDeltaAbsolute>0</SearchHalfExtentsDeltaAbsolute>
        <CanAnchorToStaticGrid>false</CanAnchorToStaticGrid>
        <EnablePreciseRotationWhenSnapped>false</EnablePreciseRotationWhenSnapped>
        <VoxelPlacement>
          <PlacementMode>OutsideVoxel</PlacementMode>
        </VoxelPlacement>
      </SmallGrid>
      <LargeStaticGrid>
        <SnapMode>Base6Directions</SnapMode>
        <SearchHalfExtentsDeltaRatio>0</SearchHalfExtentsDeltaRatio>
        <SearchHalfExtentsDeltaAbsolute>0</SearchHalfExtentsDeltaAbsolute>
        <CanAnchorToStaticGrid>true</CanAnchorToStaticGrid>
        <EnablePreciseRotationWhenSnapped>false</EnablePreciseRotationWhenSnapped>
        <VoxelPlacement>
          <PlacementMode>Both</PlacementMode>
        </VoxelPlacement>
      </LargeStaticGrid>
      <LargeGrid>
        <SnapMode>OneFreeAxis</SnapMode>
        <SearchHalfExtentsDeltaRatio>-0.1</SearchHalfExtentsDeltaRatio>
        <SearchHalfExtentsDeltaAbsolute>-0.13</SearchHalfExtentsDeltaAbsolute>
        <CanAnchorToStaticGrid>false</CanAnchorToStaticGrid>
        <EnablePreciseRotationWhenSnapped>false</EnablePreciseRotationWhenSnapped>
        <VoxelPlacement>
          <PlacementMode>InVoxel</PlacementMode>
        </VoxelPlacement>
      </LargeGrid>
    </BuildingSettings>
    <BuildingAlignIconPos x="0.5" y="0.84" /> <!-- normalized coords -->
  </Definition>
</Definitions>