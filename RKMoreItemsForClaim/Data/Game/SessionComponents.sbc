  <!--Definition for Clipboard Component (Copy/Paste functionality)-->
  <Definition xsi:type="MyObjectBuilder_ClipboardDefinition">
    <Id Type="MyObjectBuilder_ClipboardDefinition" Subtype="ClipboardBase" />

    <!--Pasting settings-->
    <PastingSettings>
      <StaticGridAlignToCenter>false</StaticGridAlignToCenter>
      <SmallStaticGrid>
        <SnapMode>OneFreeAxis</SnapMode>
        <CanAnchorToStaticGrid>true</CanAnchorToStaticGrid>
        <EnablePreciseRotationWhenSnapped>false</EnablePreciseRotationWhenSnapped>
        <!--Voxel Placement Settings: Possible settings Both,InVoxel,OutsideVoxel,Volumetric. If volumetric set than MaxAllowed and MinAllowed will be used.-->
        <VoxelPlacement>
          <PlacementMode>Both</PlacementMode>
        </VoxelPlacement>
      </SmallStaticGrid>
      <SmallGrid>
        <SnapMode>OneFreeAxis</SnapMode>
        <CanAnchorToStaticGrid>true</CanAnchorToStaticGrid>
        <EnablePreciseRotationWhenSnapped>false</EnablePreciseRotationWhenSnapped>
        <VoxelPlacement>
          <PlacementMode>Both</PlacementMode>
        </VoxelPlacement>
      </SmallGrid>
      <LargeStaticGrid>
        <SnapMode>Base6Directions</SnapMode>
        <CanAnchorToStaticGrid>true</CanAnchorToStaticGrid>
        <SearchHalfExtentsDeltaRatio>0.01</SearchHalfExtentsDeltaRatio>
        <EnablePreciseRotationWhenSnapped>false</EnablePreciseRotationWhenSnapped>
        <VoxelPlacement>
          <PlacementMode>InVoxel</PlacementMode>
        </VoxelPlacement>
      </LargeStaticGrid>
      <LargeGrid>
        <SnapMode>OneFreeAxis</SnapMode>
        <CanAnchorToStaticGrid>false</CanAnchorToStaticGrid>
        <EnablePreciseRotationWhenSnapped>false</EnablePreciseRotationWhenSnapped>
        <VoxelPlacement>
          <PlacementMode>InVoxel</PlacementMode>
        </VoxelPlacement>
      </LargeGrid>
    </PastingSettings>
  </Definition>

  <!--Definition for Local Coordinate System-->
  <Definition xsi:type="MyObjectBuilder_CoordinateSystemDefinition">
    <Id Type="MyObjectBuilder_CoordinateSystemDefinition" Subtype="CoordinateSystem" />
    <!--Angle tolerance (in deg) used for deciding if block is aligned to coord system for static grids merging.-->
    <AngleToleranceStatic>0.82</AngleToleranceStatic>
    
    <!--Angle tolerance (in deg) used for deciding if block is aligned to coord system for dynamic/unspecified grids merging.-->
    <AngleToleranceDynamic>15</AngleToleranceDynamic>
    
    <!--Position decimal count for tolerance used for deciding if block is aligned to coord system (ex. 3 - 0.001m, 5 - 0.00001m, 1 - 0.1m, etc) for static grids merging.-->
    <PositionToleranceStatic>3</PositionToleranceStatic>

    <!--Position decimal count for tolerance used for deciding if block is aligned to coord system (ex. 3 - 0.001m, 5 - 0.00001m, 1 - 0.1m, etc) for dynamic/unspecified grids merging.-->
    <PositionToleranceDynamic>3</PositionToleranceDynamic>
    
    <!--Local coordinate system size (in meters)-->
    <CoordSystemSize>500</CoordSystemSize>
  </Definition>

