    <!-- SI Classic-->
    <HorisontalTransmissionMultiplier>0.05</HorisontalTransmissionMultiplier>
    <HorisontalFragility>0.5</HorisontalFragility>
    <SupportMultiplier>2</SupportMultiplier>
  </Definition>

