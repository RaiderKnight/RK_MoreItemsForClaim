<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <!-- Basic Materials -->
  
  <Definition xsi:type="MyObjectBuilder_PhysicalMaterialDefinition">
    <Id Type="PhysicalMaterialDefinition" Subtype="Stone" />
    <Density>192.0</Density>
    <Hardness>50</Hardness>
    <!-- SI Classic-->
    <HorisontalTransmissionMultiplier>1</HorisontalTransmissionMultiplier>
    <HorisontalFragility>2</HorisontalFragility>
    <SupportMultiplier>1.4</SupportMultiplier>
    <!-- new SI prototype-->
    <BreachingStress>1.3</BreachingStress>
    <AxialStressMultiplier>1.0</AxialStressMultiplier>
    <BendingStressMultiplier>3.0</BendingStressMultiplier>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_PhysicalMaterialDefinition">
    <Id Type="PhysicalMaterialDefinition" Subtype="Wood" />
    <Density>55</Density>
    <CollisionMultiplier>1.2</CollisionMultiplier>
    <Hardness>30</Hardness>
    <!-- SI Classic-->
    <HorisontalTransmissionMultiplier>0.01</HorisontalTransmissionMultiplier>
    <HorisontalFragility>1</HorisontalFragility>
    <SupportMultiplier>1</SupportMultiplier>
    <!-- new SI prototype-->
    <BreachingStress>3</BreachingStress>
    <AxialStressMultiplier>1.2</AxialStressMultiplier>
    <BendingStressMultiplier>2.4</BendingStressMultiplier>
  </Definition>

  <Definition xsi:type="MyObjectBuilder_PhysicalMaterialDefinition">
    <Id Type="PhysicalMaterialDefinition" Subtype="Metal" />
