<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!--
  Raw foods are meant to only give a small boost to the food stat, no buffs.
  This helps new players avoid early-game death, because they don't have to forage too long.
  But it also means the basic food is not interesting to more advanced players as they don't provide buffs.
  -->

  <Definition xsi:type="MyObjectBuilder_ConsumableItemDefinition">
    <Id Type="ConsumableItem" Subtype="Berries" />
    <Tag>Berry</Tag>
    <Tag>Fruit</Tag>
    <Public>true</Public>
    <DisplayName>DisplayName_Consumables_Berries</DisplayName>
    <Description>Description_Consumables_Berries</Description>
    <Icon>Textures\GUI\Icons\Consumables\Berries.dds</Icon>
    <Size>
      <X>0.1</X>
      <Y>0.1</Y>
      <Z>0.1</Z>
    </Size>
    <Model>Models\Consumables\Berries.mwm</Model>
    <PhysicalMaterial>Food</PhysicalMaterial>
    <MaxStackAmount>30</MaxStackAmount>
    <Stats>
      <Stat Name="Food" Value="3" Clamped="true" />
      <Stat Name="Vigor" Value="-0.1" />
    </Stats>
    <UseSound>PlayEat</UseSound>
    <Mass>1</Mass>
  </Definition>
  
