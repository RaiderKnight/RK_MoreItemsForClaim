  <Definition xsi:type="MyObjectBuilder_SeedBagHandItemDefinition">
    <Id Type="SeedBagHandItem" Subtype="SeedBagPumpkin" />
	  <Tag>SeedBag</Tag>
    <DisplayName>DisplayName_Tool_SeedBagPumpkin</DisplayName>
    <Description>Description_Tool_SeedBag</Description>
    <Icon>Textures\GUI\Icons\ToolsAndWeapons\SeedBag.dds</Icon>
    <Icon>Textures\GUI\Icons\Cubes\Pumpkin.dds</Icon>
    <Size>
      <X>1.2</X>
      <Y>0.6</Y>
      <Z>0.3</Z>
    </Size>
    <Model>Models\Weapons\SeedBag_Pumpkin.mwm</Model>
    <PhysicalMaterial>Cloth</PhysicalMaterial>
    <Mass>1</Mass>
    <ItemBreakEvent>ToolBreakSeedbag</ItemBreakEvent>
    <Animation>SeedBag</Animation>
    <MaxStackAmount>1</MaxStackAmount>
    <BearerDefinition Type="MyObjectBuilder_EntityEquipmentComponent" Subtype="Humanoid" />
    <MaxDurability>10</MaxDurability>
    <Position>
      <Slot>MainHand</Slot>
    </Position>
    <Position>
      <Slot>OffHand</Slot>
    </Position>
    <!-- extra transform on IK dummy in 1st person -->
    <EquippedTransform>
      <!-- tool rotation offset from hand -->
      <Position>
        <X>0</X>
        <Y>0</Y>
        <Z>0.2</Z>
      </Position>
      <Orientation>
        <X>0.707</X>
        <Y>0</Y>
        <Z>0</Z>
        <W>-0.707</W>
      </Orientation>
    </EquippedTransform>
    <EquippedTransformFps>
      <!-- hand IK target offset -->
      <Position>
        <X>0.360</X>
        <Y>-0.258</Y>
        <Z>-0.339</Z>
      </Position>
      <Orientation>
        <X>0.3001874</X>
        <Y>0.6298291</Y>
        <Z>-0.2882942</Z>
        <W>0.6558119</W>
      </Orientation>
    </EquippedTransformFps>
    <!-- Behavior -->
    <StanceToBehavior>
      <StanceName>NormalMode</StanceName>
      <BehaviorId Type="MyObjectBuilder_SeedingToolBehaviorDefinition" Subtype="SeedingNormal" />
    </StanceToBehavior>
    <!-- Seed bag definition -->
    <GrowableDefinitionId>
      <TypeId>MyObjectBuilder_FarmableEnvironmentItemDefinition</TypeId>
      <SubtypeId>FarmPumpkin</SubtypeId>
    </GrowableDefinitionId>
  </Definition>

